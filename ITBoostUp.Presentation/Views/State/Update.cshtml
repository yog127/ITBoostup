@model ITBoostUp.BusinessLayer.Model.State

<h1>Upadte State</h1>
<form class="form-control" method="post">
    <div><input type="number" class="form-control" value="@Model.StateId" asp-for="StateId" readonly /></div>
    
    <div>
        <label>State Name</label>
        <input type="text" class="form-control" value="@Model.StateName" asp-for="StateName" />
    </div>
    <div>
        <label>StateDescription</label>
        <input type="text" class="form-control" value="@Model.StateDescription" asp-for="StateDescription" />
    </div>
    <div>
        <label>UpdateBy</label>
        <input asp-for="UpdatedBy" class="form-control" />
    </div>
    <input type="submit" class="btn btn-danger" value="Submit" />
</form>

